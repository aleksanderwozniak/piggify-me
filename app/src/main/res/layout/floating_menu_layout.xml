<?xml version="1.0" encoding="utf-8"?>
<com.getbase.floatingactionbutton.FloatingActionsMenu
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    android:id="@+id/fab"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    fab:fab_labelsPosition="left"
    fab:fab_labelStyle="@style/menu_labels_style"
    fab:fab_addButtonSize="normal"
    fab:fab_addButtonStrokeVisible="false"
    fab:fab_addButtonPlusIconColor="@color/black"
    fab:fab_addButtonColorNormal="@color/white"
    fab:fab_addButtonColorPressed="@color/whitePressed">

    <com.getbase.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_income"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onFabIncomeClick"
        fab:fab_colorNormal="@color/white"
        fab:fab_colorPressed="@color/whitePressed"
        fab:fab_icon="@drawable/ic_arrow_up"
        fab:fab_size="mini"
        fab:fab_stroke_visible="false"
        fab:fab_title="Add new income" />

    <com.getbase.floatingactionbutton.FloatingActionButton
        android:id="@+id/fab_expense"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onFabExpenseClick"
        fab:fab_colorNormal="@color/white"
        fab:fab_colorPressed="@color/whitePressed"
        fab:fab_icon="@drawable/ic_arrow_down"
        fab:fab_size="mini"
        fab:fab_stroke_visible="false"
        fab:fab_title="Add new expense" />

</com.getbase.floatingactionbutton.FloatingActionsMenu>